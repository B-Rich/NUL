# -*- Mode: Python -*-

import nul

Import('target_env')

nul.App(target_env, 'cycleburner',
        SOURCES = [ 'cycleburner.cc'],
        INCLUDE = [ 'sigma0' ],
        OBJS    = [ '#service/dlmalloc.o',
                    '#service/dlmalloc-support.o',
                    '#service/simplemalloc.o',
                    '#service/logging.o',
                    '#service/vprintf.o',
                    ],
        LIBS    = [ 'runtime' ],
        MEMSIZE = 1<<20)

# EOF
